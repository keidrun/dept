{
  "name": "dept",
  "version": "0.0.21",
  "description": "Dependencies templates management CLI to install your fixed NPM or Yarn dependencies and cofing files to your project.",
  "bin": {
    "dept": "build/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "dept",
    "cli",
    "template",
    "npm",
    "yarn",
    "dependencies",
    "devDependencies",
    "config"
  ],
  "repository": "https://github.com/keidrun/dept.git",
  "author": "Keid",
  "license": "MIT",
  "dependencies": {
    "semver": "^5.6.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2"
  },
  "lint-staged": {
    "src/**/**.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.js$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "scripts": {
    "dev": "NODE_ENV='development' babel-node src/index.js",
    "clean": "rimraf build",
    "build": "yarn clean && babel src --out-dir build",
    "prod": "node build/index.js",
    "test": "jest --coverage",
    "lint": "eslint --fix 'src/**/*.js'",
    "precommit": "lint-staged"
  }
}
