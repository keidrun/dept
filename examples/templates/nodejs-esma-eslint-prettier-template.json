{
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "files": {
    ".eslintrc": {
      "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:jest/recommended"
      ],
      "plugins": [
        "prettier",
        "jest"
      ],
      "parser": "babel-eslint",
      "rules": {
        "prettier/prettier": [
          "error",
          {
            "trailingComma": "es5",
            "singleQuote": true,
            "semi": false,
            "printWidth": 120
          }
        ],
        "no-unused-vars": [
          "warn",
          {
            "vars": "all"
          }
        ],
        "arrow-body-style": [
          "error",
          "as-needed"
        ],
        "no-console": "off",
        "radix": "warn",
        "no-shadow": [
          "error",
          {
            "builtinGlobals": false,
            "hoist": "all",
            "allow": [
              "resolve",
              "reject",
              "done",
              "next",
              "err",
              "error",
              "cb",
              "callback"
            ]
          }
        ],
        "quotes": [
          "error",
          "single",
          {
            "avoidEscape": true,
            "allowTemplateLiterals": true
          }
        ],
        "no-underscore-dangle": "off"
      }
    }
  }
}
